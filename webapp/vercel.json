{
  "version": 2,
  "name": "marketplace",
  "build": {
    "env": {
      "GEN_STATIC_LOCAL": "true",
      "VITE_REACT_APP_DCL_DEFAULT_ENV": "prod",
      "NODE_OPTIONS": "--max-old-space-size=8192"
    }
  },
  "github": {
    "silent": true
  },
  "rewrites": [
    {
      "source": "/auth/:match*",
      "destination": "https://decentraland.zone/auth/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
